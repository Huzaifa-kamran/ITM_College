
@{
	Layout = "_FacultyLayout";
}
@model IEnumerable<Course>;
<!--**********************************
		  Content body start
	  ***********************************-->
<div class="content-body">
	@if (ViewBag.message != null)
	{

		<div class="alert alert-success solid alert-right-icon alert-dismissible fade show">
			<span><i class="mdi mdi-check"></i></span>
			<button type="button" class="btn-close" data-bs-dismiss="alert" aria-label="btn-close">
			</button> @ViewBag.message
		</div>
	}

	@if (ViewBag.error != null)
	{

		<div class="alert alert-danger solid alert-right-icon alert-dismissible fade show">
			<span><i class="mdi mdi-check"></i></span>
			<button type="button" class="btn-close" data-bs-dismiss="alert" aria-label="btn-close">
			</button> @ViewBag.error
		</div>
	}
	<!-- row -->
	<div class="container-fluid">

		<div class="row page-titles mx-0">
			<div class="col-sm-6 p-md-0">
				<div class="welcome-text">
					<h4>All Professors</h4>
				</div>
			</div>
			<div class="col-sm-6 p-md-0 justify-content-sm-end mt-2 mt-sm-0 d-flex">
				<ol class="breadcrumb">

					<li class="breadcrumb-item"><a href="javascript:void(0);">Courses</a></li>
					<li class="breadcrumb-item active"><a href="javascript:void(0);">All Courses</a></li>
				</ol>
			</div>
		</div>

		<div class="row">
			<div class="col-lg-12">
				<ul class="nav nav-pills mb-3">
					<li class="nav-item"><a href="#list-view" data-bs-toggle="tab" class="nav-link me-1 show active">List View</a></li>
					<li class="nav-item"><a href="#grid-view" data-bs-toggle="tab" class="nav-link">Grid View</a></li>
				</ul>
			</div>
			<div class="col-lg-12">
				<div class="row tab-content">
					<div id="list-view" class="tab-pane fade active show col-lg-12">
						<div class="card">
							<div class="card-header">
								<h4 class="card-title">All Courses</h4>
								<a asp-controller="Admin" asp-action="AddStudent" class="btn btn-primary">+ Add new</a>
							</div>
							<div class="card-body">
								@if (Model.Any())
								{


									<div class="table-responsive">
										<table id="example3" class="display text-nowrap table" style="min-width: 845px">
											<thead>
												<tr>
													<th>Image</th>
													<th>Name</th>
													<th>Faculty</th>
													<th>Department</th>
													<th>Duration</th>
													<th>Description</th>
													<th>Action</th>
												</tr>
											</thead>
											<tbody>
												@foreach (var temp in Model)
												{
													<tr>
														<td><img class="rounded-circle" width="35" src="~/@temp.CourseImg" alt=""></td>
														<td>@temp.CourseName</td>
														<td>@temp.Faculty.FacultyName</td>
														<td>@temp.Faculty.FacultyDepartmentNavigation.DepartmentName</td>
														<td>@temp.CourseDuration</td>
														<td>@temp.CourseDesc</td>
														<td>
															<a asp-controller="Admin" asp-action="UpdateCourse" asp-route-id="@temp.CourseId" class="btn btn-xs sharp btn-primary"><i class="fa fa-pencil"></i></a>
															<a asp-controller="Admin" asp-action="DeleteCourse" asp-route-id="@temp.CourseId" class="btn btn-xs sharp btn-danger"><i class="fa fa-trash"></i></a>
														</td>
													</tr>
												}



											</tbody>
										</table>
									</div>
								}
								else
								{
									<h2>No Course Found</h2>
								}
							</div>
						</div>
					</div>
